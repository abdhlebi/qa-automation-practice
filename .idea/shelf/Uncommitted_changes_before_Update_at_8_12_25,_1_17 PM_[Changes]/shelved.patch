+from selenium import webdriver
+from selenium.webdriver.common.by import By
+import time
+
+def youtube_search():
+
+    driver = webdriver.Chrome()
+    driver.get("https://www.youtube.com")
+    time.sleep(10)
+
+    search_box = driver.find_element(By.XPATH, '//*[@id="center"]/yt-searchbox/div[1]/form/input')
+    search_box.send_keys("QA Automation with Python")
+
+    search_buttom = driver.find_element(By.XPATH, '//*[@id="center"]/yt-searchbox/button/span/span/div')
+    search_buttom.click()
+    time.sleep(15)
+
+    assert "qa automation" in driver.title.lower(), "Search term not found in title"
+
+    driver.quit()
+
+youtube_search()
